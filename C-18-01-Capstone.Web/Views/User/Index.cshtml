
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@model  string

@Styles.Render("~/bundles/profile")

@using (Html.BeginForm())
{
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-6 col-md-6">
                <div class="well well-sm">
                    <div class="row">
                        <div class="col-sm-6 col-md-4">
                            <img src="https://i.pinimg.com/736x/26/b7/04/26b704ca46361633d9134e397bf0f8f1--hugh-jackman-hugh-obrian.jpg" alt="" class="img-rounded img-responsive" />
                        </div>
                        <div class="col-sm-6 col-md-8" id="profileData">
                            @Html.Hidden("LoginHidden", Model, new { @id = "login" })
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}

@Scripts.Render("~/bundles/profileJs")